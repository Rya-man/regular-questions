#include<iostream>
#include<iomanip>
#include<math.h>
using namespace std;
int ptrav(unsigned long r,int a)
{
    int flag, k[a];
    for (int i = 2; i < r; i++)
    {
        
        flag = 1;
        for (int j = 2; j <= i / 2; ++j)
        {
            if (i % j == 0) 
            {
                flag = 0;
                break;
            }
        }
        if(flag==1)
        {   int l;
            for(l=0;l<a;l++)
            {
                k[l]=i;
               
            }
        
        }
    }
    return k[a-1];
}
bool CheckPrime(unsigned long n) 
{ 
    unsigned long i;int flag = 1; 
    for(i = 2; i < n; i++) 
    { 
        if(n % i == 0) 
          { 
              flag = 0;  
              return 0; 
              break;
          } 
 
    } 
    if(flag == 1) 
        return 1; 
}
int f(unsigned long n)
{   int af;
    for(int i=0;i<n;i++)
    {   af=0;
        if(CheckPrime(n-ptrav(n,i))==1)
        {
            af=1;
            break;
        }
        else
        {
            continue;
        }
    }
    af==1?cout<<"Yes":cout<<"No";
}
int main()
{
    unsigned long n;
    cout<<"\n\nEnter number\n\n";
    cin>>n;
    f(n);
    return 0;
}
